  <!-- Control Statements ('else' on new line) -->
  <setting id="org.eclipse.jdt.core.formatter.keep_then_statement_on_same_line" value="false" />
  <setting id="org.eclipse.jdt.core.formatter.keep_else_statement_on_same_line" value="false" /> <!--
  This forces 'else' onto a new line -->
  <setting id="org.eclipse.jdt.core.formatter.keep_empty_array_initializer_on_one_line"
    value="false" />
  <setting id="org.eclipse.jdt.core.formatter.keep_simple_if_on_one_line" value="false" />
  <setting id="org.eclipse.jdt.core.formatter.keep_guardian_clause_on_one_line" value="false" />
  <setting id="org.eclipse.jdt.core.formatter.keep_then_statement_on_same_line" value="true" />

  <!-- Specifically allow keeping the entire 'if (cond) statement;' on one line if it's simple -->
