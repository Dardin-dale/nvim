<profile kind="CodeFormatterProfile" name="CustomJava120" version="21"> <!-- Choose a name, version
  can be updated -->
  <!-- Profile Settings -->
  <setting id="org.eclipse.jdt.core.formatter.profile" value="CustomJava120" />

  <!-- Line Wrapping -->
  <setting id="org.eclipse.jdt.core.formatter.lineSplit" value="120" /> <!-- Set desired line width -->
  <setting id="org.eclipse.jdt.core.formatter.comment.line_length" value="120" />

  <!-- Indentation -->
